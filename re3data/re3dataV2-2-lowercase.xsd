  <xs:element name="re3data" msdata:prefix="r3d">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="repository" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="re3data.orgidentifier" msdata:prefix="r3d" type="xs:string" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>a globally unique and persistent identifier that refers to the research data repository entry in re3data.dorg. the id is assigned by re3data.org.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="repositoryname" msdata:prefix="r3d" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>the full name of the research data repository.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute name="language" msdata:prefix="r3d" type="languages" form="unqualified" use="required">
                        <xs:annotation>
                          <xs:documentation>language of the research data repository name.</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="additionalname" msdata:prefix="r3d" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>the alternative name or acronym for the research data repository.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute name="language" msdata:prefix="r3d" type="languages" form="unqualified" use="required">
                        <xs:annotation>
                          <xs:documentation>language of the research data repository additional name.</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="repositoryurl" msdata:prefix="r3d" type="xs:anyURI" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>the url, which gives reference to the research data repository.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="repositoryidentifier" msdata:prefix="r3d" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>a globally unique identifier (identifier in form of a link) that refers to the research data repository (e.g. doi, urn, viaf, gnd).</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="description" msdata:prefix="r3d" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>a textual description containing additional information about the data repository (primary language is english).</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="stringl1000">
                      <xs:attribute name="language" msdata:prefix="r3d" type="languages" form="unqualified" use="required">
                        <xs:annotation>
                          <xs:documentation>language of the research data repository description.</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="repositorycontact" msdata:prefix="r3d" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>email address of the contact or an url of an online contact form of the repository.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="type" msdata:prefix="r3d" type="repositorytypes" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>the type of the research data repository.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="size" msdata:prefix="r3d" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>the number of items contained in the research data repository.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute name="updated" msdata:prefix="r3d" type="dateFormat" use="required" form="unqualified">
                        <xs:annotation>
                          <xs:documentation>the date of the last update of the research data repository size.</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="startdate" msdata:prefix="r3d" type="dateFormat" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>releasing date of the research data repository.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="enddate" msdata:prefix="r3d" type="dateFormat" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>date when the research data repository went offline or stopped ingesting new research data (still making the research data available).</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="repositorylanguage" msdata:prefix="r3d" type="languages" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>the user interface language of the research data repository.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="subject" msdata:prefix="r3d" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>the subject(s) of the research data repository.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="subject_text">
                      <xs:attribute name="subjectScheme" form="unqualified" use="required">
                        <xs:annotation>
                          <xs:documentation>the subject scheme according to which the subject (see id 13 subject) of the research data repository described.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:enumeration value="DFG" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="missionstatementurl" msdata:prefix="r3d" type="xs:anyURI" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                  <xs:documentation>the url of a mission statement describing the designated community of the research data repository.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="contenttype" msdata:prefix="r3d" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>all types of resources available in the research data repository.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="contenttype_text">
                      <xs:attribute name="contenttypescheme" form="unqualified" use="required">
                        <xs:annotation>
                          <xs:documentation>the name and/or url of the subject scheme or classification code (e.g.parse).</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:enumeration value="parse" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="providertype" msdata:prefix="r3d" type="providertypes" minOccurs="1" maxOccurs="2">
                <xs:annotation>
                  <xs:documentation>the type of provider.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="keyword" msdata:prefix="r3d" type="xs:string" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>english keyword(s) describing the subject focus of the research data repository.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="institution" msdata:prefix="r3d" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                  <xs:documentation>all institutions being responsible for funding, creating and/or running the research data repository (wrapper element).</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="institutionname" msdata:prefix="r3d" minOccurs="1" maxOccurs="1">
                      <xs:annotation>
                        <xs:documentation>name of the responsible institution.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute name="language" msdata:prefix="r3d" type="languages" form="unqualified" use="required">
                              <xs:annotation>
                                <xs:documentation>language of the institution name.</xs:documentation>
                              </xs:annotation>
                            </xs:attribute>
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="institutionadditionalname" msdata:prefix="r3d" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation>the alternative name or acronym for the responsible institution.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute name="language" msdata:prefix="r3d" type="languages" form="unqualified" use="required">
                              <xs:annotation>
                                <xs:documentation>language of the institution additional name.</xs:documentation>
                              </xs:annotation>
                            </xs:attribute>
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="institutioncountry" msdata:prefix="r3d" type="countries" minOccurs="1" maxOccurs="1">
                      <xs:annotation>
                        <xs:documentation>location of the responsible institution.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="responsibilitytype" msdata:prefix="r3d" type="responsibilitytypes" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation>type of responsibility for each responsible institution.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="institutiontype" msdata:prefix="r3d" type="institutiontypes" minOccurs="0" maxOccurs="1">
                      <xs:annotation>
                        <xs:documentation>type of responsible institution.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="institutionurl" msdata:prefix="r3d" type="xs:anyURI" minOccurs="0" maxOccurs="1">
